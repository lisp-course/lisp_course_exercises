<package>
  <name>cram_execution_trace</name>
  <version>0.7.0</version>
  <description>
    The execution trace provides facilities for tracing all state changes
    during plan execution, serializing this episode information and accessing
    it in various ways.
  </description>

  <author>Nikolaus Demmel</author>
  <author>Lorenz Moesenlechner</author>

  <maintainer email="kazhoyan@cs.uni-bremen.de">Gayane Kazhoyan</maintainer>

  <license>BSD</license>

  <url type="website">http://cram-system.org</url>
  <url type="bugtracker">https://github.com/cram2/cram_core/issues</url>
  <url type="repository">https://github.com/cram2/cram_core</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>sbcl</build_depend>
  <build_depend>cl_store</build_depend>
  <build_depend>cram_utilities</build_depend>
  <build_depend>cram_test_utilities</build_depend>
  <build_depend>cram_language</build_depend>

  <run_depend>sbcl</run_depend>
  <run_depend>cl_store</run_depend>
  <run_depend>cram_utilities</run_depend>
  <run_depend>cram_test_utilities</run_depend>
  <run_depend>cram_language</run_depend>
</package>
